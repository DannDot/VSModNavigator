{
  "name": "vs-mod-navigator",
  "displayName": "Vintage Story Mod Navigator",
  "description": "Navigate between Vintage Story mod files and JSON patches.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "json",
        "extensions": [
          ".json"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "jsonc-parser": "^3.2.0"
  }
}
